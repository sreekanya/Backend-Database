<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>FitnessCenter</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>

  </welcome-file-list>
  
  <servlet>
  	<description></description>
  	<display-name>BatchServlet</display-name>
  	<servlet-name>BatchServlet </servlet-name>
  	<servlet-class>com.YaminGym.FitnessCenter.controller.BatchServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>BatchServlet</servlet-name>
  	<url-pattern>/BatchServlet</url-pattern>
  	<url-pattern>/Batch</url-pattern>
  </servlet-mapping>
  
</web-app>